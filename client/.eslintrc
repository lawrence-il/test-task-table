{
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "react-app"
    ],
    "rules": {
        "no-console": "warn",
        "semi": ["warn", "always"],
        "import/order": ["warn", {
            "groups": [
                "builtin", 
                "external", 
                "internal", 
                "parent", 
                "sibling", 
                "index", 
                "object", 
                "type"
            ],
            "newlines-between": "always-and-inside-groups"
        }]

    }
}